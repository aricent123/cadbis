<catalog name="example">

  <chain     				name="authorize">
    <mon-req				name="authorize-mon-req"
		  		 description="Answers monitoring requests with a reject"
		  		    username="monitoring"
		  		       nasid="monitoring"
		  		replyMessage="Monitoring request acknowledged"/>
    <!--tlstunnel-init			name="authorize-tunnel-init"
		  		 description="Initialize Possible Tunnel Session"/-->
    <init-session			name="authorize-init-session"
		  		 description="Initialize The Radius Session"/>
    <!--tlstunnel-auth			name="authorize-tunnel-auth"
				 description="Handle Authorization for Terminated Tunnels"
				anonUserName="anonymous"/-->
	<otp-proxy-auth			name="authorize-otp-proxy-auth"
				 description="OTP Proxy Authentication"/>
  </chain>

  <chain     				name="post_auth">
    <mon-req				name="authorize-mon-req"
				 description="Answers monitoring requests with a reject"/>
    <init-session			name="post_auth-init-session"
				 description="Initialize The Radius Session"/>
    <check-standard			name="check-irap-standard"
		  		 description="Check IRAP Standards"
		  		standardName="net.jradius.standard.IRAPStandard"/>
	<otp-proxy-post-auth		name="authorize-otp-proxy-auth"
				 description="OTP Proxy Post Authentication"/>
	<command className="net.jradius.example.WPACaptivePortal" />
    <class-post-auth			name="post_auth-class"
		  		 description="Post-Auth Class Attribute Handler"/>
  </chain>

  <chain  				name="preacct">
    <!--tlstunnel-init			name="authorize-tunnel-init"
		  		 description="Initialize Possible Tunnel Session"/-->
    <init-session			name="preacct-init-session"
				 description="Initialize The Radius Session"/>
    <check-standard			name="check-irap-standard"
	 			 description="Check IRAP Standards"
	 		        standardName="net.jradius.standard.IRAPStandard"/>
    <class-preacct			name="preacct-class"
		  		 description="Accounting Class Attribute Handler"/>
    <!--tlstunnel-preacct			name="preacct-tunnel-pre-acct"
   				 description="Handle Pre-Accounting for Terminated Tunnels"/-->
  </chain>

  <chain    				name="start_accounting">
    <init-session			name="accounting-init-session"
				 description="Initialize The Radius Session"/>
  </chain>

  <chain     				name="interim_accounting">
    <init-session			name="accounting-init-session"
				 description="Initialize The Radius Session"/>
  </chain>

  <chain    				name="stop_accounting">
    <init-session			name="accounting-init-session"
				 description="Initialize The Radius Session"/>
  </chain>

  <chain    				name="other_accounting">
    <init-session			name="accounting-init-session"
				 description="Initialize The Radius Session"/>
  </chain>

  <chain    				name="pre_proxy">
    <init-session			name="pre_proxy-init-session"
				 description="Initialize The Radius Session"/>
    <class-proxy			name="pre_proxy-class"
		  		 description="Pre-Proxy Class Attribute Handler"/>
    <rewrite-ses-id			name="pre_proxy-rewrite-ses-id"
 	 	 		 description="Rewrite the Acct-Session-Id to be the RadiusSession SessionKey"/>
    <check-standard			name="check-irap-standard"
		   		 description="Check IRAP Standards"
				standardName="net.jradius.standard.IRAPStandard"/>
  </chain>

  <chain 		    		name="post_proxy">
    <init-session			name="post_proxy-init-session"
				 description="Initialize The Radius Session"/>
    <check-standard			name="check-irap-standard"
		  		 description="Check IRAP Standards"
		  		standardName="net.jradius.standard.IRAPStandard"/>
  </chain>

</catalog>
